{
  "delimiter":",",
  "fields":[
    {
      "name":"centerStatus",
      "where":[
        {"step.name":"getCenterState"}
      ],
      "select":"body",
      "map":"identity",
      "agg":"last",
      "dflt":"open"
    },
    {
      "name":"language",
      "select":"flow.language",
      "map":"identity",
      "agg":"last",
      "dflt":"English"
    },
    {
      "name":"timeInMenu",
      "where":[
        {"step.stepClass":"GatherInput"}
      ],
      "select":"step.duration",
      "map":"identity",
      "agg":"sum",
      "dflt":0
    },
    {
      "name":"timeInSayPlay",
      "where":[
        {"step.stepClass":["SayOrPlay"]}
      ],
      "select":"step.duration",
      "map":"identity",
      "agg":"sum",
      "dflt":0
    },
    {
      "name":"maxTimeInStep",
      "select":"step.duration",
      "map":"identity",
      "agg":"max",
      "dflt":0
    },
    {
      "name":"functionCallCnt",
      "where":[
        {"step.stepClass":["HttpCall"]}
      ],
      "select":1,
      "map":"identity",
      "agg":"count",
      "dflt":0
    },
    {
      "name":"abandonCnt",
      "where":[
        {"QueueResult":"hangup"}
      ],
      "select":"step.idx",
      "map":"identity",
      "agg":"count",
      "dflt":0
    }
  ],
  "batchSize":5
}
