{
  "delimiter":",",
  "fields":[
    {
      "name":"timeInMenu",
      "where":[
        {"step.stepClass":"GatherInput"}
      ],
      "select":"step.duration",
      "agg":"sum",
      "dflt":0
    },
    {
      "name":"timeInSayPlay",
      "where":[
        {"step.stepClass":["SayOrPlay"]}
      ],
      "select":"step.duration",
      "agg":"sum",
      "dflt":0
    },
    {
      "name":"maxTimeInStep",
      "select":"step.duration",
      "agg":"max",
      "dflt":0
    },
    {
      "name":"functionCallCnt",
      "where":[
        {"step.stepClass":["HttpCall"]}
      ],
      "select":1,
      "agg":"count",
      "dflt":0
    },
    {
      "name":"abandonCnt",
      "where":[
        {"QueueResult":"hangup"}
      ],
      "select":"step.idx",
      "agg":"count",
      "dflt":0
    }
  ],
  "batchSize":5
}